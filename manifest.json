{
  "name": "Close Whatsapp Chat",
  "version": "1.0",
  "description": "Allows you to close a chat on whatsapp web",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}